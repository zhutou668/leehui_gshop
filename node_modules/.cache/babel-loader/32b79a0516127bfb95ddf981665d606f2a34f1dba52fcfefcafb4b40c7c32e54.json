{"ast":null,"code":"import { mapActions } from 'vuex';\nimport { mapState } from 'vuex';\nimport AppHeaderTop from \"../../components/HeaderTop/AppHeaderTop.vue\";\nimport { Swiper, SwiperSlide } from 'swiper/vue';\nimport 'swiper/swiper-bundle.css'; // Import CSS của Swiper\n\nexport default {\n  name: 'AppMsite',\n  components: {\n    AppHeaderTop,\n    Swiper,\n    SwiperSlide\n  },\n  data() {\n    return {\n      swiperOptions: {\n        slidesPerView: 1,\n        spaceBetween: 30,\n        loop: true,\n        // Cho phép lặp lại\n        navigation: {\n          nextEl: '.swiper-button-next',\n          prevEl: '.swiper-button-prev'\n        }\n      },\n      images: ['https://example.com/image1.jpg', 'https://example.com/image2.jpg'\n      // Thêm nhiều hình ảnh khác nếu cần\n      ]\n    };\n  },\n\n  mounted() {\n    //   this.$store.dispatch('getPosts')\n    //   console.log(this.posts)\n    // this.$store.dispatch('getProfile')\t\n    this.getCatergorys();\n    this.getAddress();\n  },\n  methods: {\n    ...mapActions(['getCatergorys', 'getAddress'])\n  },\n  computed: {\n    ...mapState(['address', 'catergorys'])\n  }\n};","map":{"version":3,"names":["mapActions","mapState","AppHeaderTop","Swiper","SwiperSlide","name","components","data","swiperOptions","slidesPerView","spaceBetween","loop","navigation","nextEl","prevEl","images","mounted","getCatergorys","getAddress","methods","computed"],"sources":["src/pages/Msite/AppMsite.vue"],"sourcesContent":["<template>\n    <div class=\"AppMsite\">\n      <AppHeaderTop :title=\"address.address\">\n      <span class=\"header_left\" slot=\"header_left\"><font-awesome-icon icon=\"fa-solid fa-magnifying-glass\" size:50px/></span>\n      <span class=\"header_right\" slot=\"header_right\">登录/注册</span>\n   </AppHeaderTop>\n   <div class=\"Msite_Menu\">\n      <Swiper :options=\"swiperOptions\">\n      <SwiperSlide v-for=\"(image, index) in images\" :key=\"index\">\n        <img :src=\"image\" :alt=\"Slide {{ index +