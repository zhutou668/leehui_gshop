  const onSwiper = (swiper) => {\n            console.log(swiper);\n         };\n         const onSlideChange = () => {\n            console.log('slide change');\n         };\n\n         return {\n            onSwiper,\n            onSlideChange,\n            swiper, // Expose the Swiper instance to the template\n         };\n         },\n   }\n \n  </script>\n  \n  <style scoped>\n\n  \n  </style>\n  "],"mappings":"AA6BA,SAAAA,UAAA;AACA,SAAAC,QAAA;AACA,OAAAC,YAAA;AAEA,OAAAC,MAAA;AACA,SAAAC,UAAA,EAAAC,UAAA,EAAAC,SAAA,EAAAC,IAAA;;AAEA;AACA;AACA;AACA;AACA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAP,YAAA;IACAC,MAAA;IACAO;EACA;EACAC,QAAA;IACA;IACA;IACA;IACA,KAAAC,aAAA;IACA,KAAAC,UAAA;EACA;EAEAC,OAAA;IACA,GAAAd,UAAA;EACA;EACAe,QAAA;IACA,GAAAd,QAAA;EACA;EACAe,MAAA;IACA,MAAAC,MAAA,OAAAd,MAAA;MACAe,OAAA,GAAAd,UAAA,EAAAC,UAAA,EAAAC,SAAA,EAAAC,IAAA;MACAY,aAAA;MACAC,YAAA;MACAC,UAAA;MACAC,UAAA;QAAAC,SAAA;MAAA;MACAC,SAAA;QAAAC,SAAA;MAAA;IACA;IAEA,MAAAC,QAAA,GAAAT,MAAA;MACAU,OAAA,CAAAC,GAAA,CAAAX,MAAA;IACA;IACA,MAAAY,aAAA,GAAAA,CAAA;MACAF,OAAA,CAAAC,GAAA;IACA;IAEA;MACAF,QAAA;MACAG,aAAA;MACAZ,MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}